<xmlui>
  <strip id="stripMain">
    <section id="sectionAdd">
      <action id="addDescription"/>
    </section>
    <section id="sectionProject">
      <action id="updateProject"/>
    </section>
    <section id="sectionEdit">
      <grid>
        <action id="editCopy"/>
        <action id="find"/>
      </grid>
    </section>
    <section id="sectionReport">
      <grid>
        <action id="printReport"/>
        <action id="popupExport"/>
      </grid>
    </section>
  </strip>
  <menu id="menuLink">
    <action id="openLink"/>
    <action id="copyLink"/>
  </menu>
  <menu id="menuLinkItem">
    <action id="gotoLinkItem"/>
  </menu>
  <menu id="menuLinkEmail">
    <action id="sendEmail"/>
    <action id="copyEmail"/>
  </menu>
  <menu id="menuSelection">
    <action id="editCopy"/>
    <separator/>
    <action id="editSelectAll"/>
  </menu>
  <menu id="menuSummary">
    <action id="addDescription"/>
    <separator/>
    <action id="editSelectAll"/>
    <separator/>
    <action id="find"/>
    <action id="findNext"/>
    <action id="findPrevious"/>
  </menu>
  <menu id="menuExport">
    <action id="exportPdf"/>
    <action id="exportHtml"/>
  </menu>
</xmlui>
