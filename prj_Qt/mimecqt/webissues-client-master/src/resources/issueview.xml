<xmlui>
  <strip id="stripMain">
    <section id="sectionAdd">
      <action id="addComment"/>
      <action id="addAttachment"/>
      <action id="addDescription"/>
    </section>
    <section id="sectionIssue">
      <action id="updateIssue"/>
      <action id="editIssue"/>
      <action id="cloneIssue"/>
      <grid>
        <action id="moveIssue"/>
        <action id="deleteIssue"/>
        <action id="markAsRead"/>
        <action id="subscribe"/>
      </grid>
    </section>
    <section id="sectionEdit">
      <grid>
        <action id="editCopy"/>
        <action id="find"/>
      </grid>
    </section>
    <section id="sectionReport">
      <grid>
        <action id="printReport"/>
        <action id="popupExport"/>
      </grid>
    </section>
  </strip>
  <menu id="menuLink">
    <action id="openLink"/>
    <action id="copyLink"/>
  </menu>
  <menu id="menuLinkItem">
    <action id="gotoLinkItem"/>
  </menu>
  <menu id="menuLinkAttachment">
    <action id="openAttachment"/>
    <action id="saveAttachment"/>
  </menu>
  <menu id="menuLinkEmail">
    <action id="sendEmail"/>
    <action id="copyEmail"/>
  </menu>
  <menu id="menuSelection">
    <action id="editCopy"/>
    <separator/>
    <action id="editSelectAll"/>
  </menu>
  <menu id="menuHistory">
    <action id="editIssue"/>
    <separator/>
    <action id="addComment"/>
    <action id="addAttachment"/>
    <action id="addDescription"/>
    <separator/>
    <action id="markAsRead"/>
    <action id="subscribe"/>
    <separator/>
    <action id="editSelectAll"/>
    <separator/>
    <action id="find"/>
    <action id="findNext"/>
    <action id="findPrevious"/>
  </menu>
  <menu id="menuExport">
    <action id="exportPdf"/>
    <action id="exportHtml"/>
  </menu>
</xmlui>
